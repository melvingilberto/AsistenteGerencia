<configuration>

    <system.web>
      <compilation debug="true" targetFramework="4.5.2" />
      <httpRuntime targetFramework="4.5.2" />
      <customErrors mode="RemoteOnly" defaultRedirect="index.aspx"/>
    </system.web>
    <system.webServer>
      <defaultDocument>
        <files>
          <add value="index.aspx" />
        </files>
      </defaultDocument>
      <directoryBrowse enabled="true" />
    </system.webServer>

  <connectionStrings>
    <add name="DBAG" connectionString="server=.; database=asistente_de_gerencias; integrated security = SSPI"
      providerName="System.Data.SqlClient" />
    <add name="asistente_de_gerenciasConnectionString" connectionString="Data Source=.;Initial Catalog=asistente_de_gerencias;Integrated Security=True"
      providerName="System.Data.SqlClient" />
  </connectionStrings>
  
    <appSettings>
      <add key="ValidationSettings:UnobtrusiveValidationMode" value="None" />
    </appSettings>

</configuration>
